{
  "name": "@app/renderer-entrypoint",
  "version": "0.0.1",
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "scripts": {
    "build": "yarn next build src --no-lint && rm ./src/.next/standalone/package.json ; cp -r ./src/.next/standalone ./dist && cp -r ./src/.next/static ./dist/src/.next/static && mv ./dist/src/server.js ./dist/src/index.cjs",
    "dev": "NEXT_PRIVATE_LOCAL_WEBPACK=true yarn next dev src",
    "prepack": "run build",
    "start": "node dist/src/index.cjs"
  },
  "dependencies": {
    "@module-federation/nextjs-mf": "8.4.9",
    "@vanilla-extract/dynamic": "2.1.1",
    "@vanilla-extract/next-plugin": "2.4.3",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "styled-jsx": "5.1.6"
  },
  "devDependencies": {
    "@app/index-page": "workspace:0.0.1",
    "@types/node": "22.2.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "typescript": "5.2.2",
    "webpack": "5.93.0"
  },
  "packConfiguration": {
    "buildpackVersion": "0.0.5"
  }
}
